<div class="utils">
  <% unless @meta.layers.first.nil? %>
    <% @layers = @meta.layers.map{|node|node.text} %>
    <span class="kml-link">
      <%= link_to(kml_url(@layers.join(',').chomp ), 'Google Earth') %>
    </span>
  <% end %>

  <% unless @meta.catalog_link.nil? %>
    <span class="catalog-link">
    <%= link_to(cat_url(@meta.catalog_link.content), 'Virgo') %>
  </span>
<% end %>
</div>
